{
	"name": "68publishers/doctrine-bridge",
	"description": "Bridges between integrations of Doctrine ORM and '68publishers bundles.",
	"keywords": ["68publishers", "doctrine", "orm", "bridge", "nette"],
	"license": "MIT",
	"authors": [
		{
			"name": "Tomáš Glawaty",
			"email": "tomasglawaty@icloud.com"
		}
	],
	"require": {
		"php": "^8.1",
		"nette/di": "^3.0.10",
		"doctrine/orm": "^2.10.0"
	},
	"require-dev": {
		"bamarni/composer-bin-plugin": "^1.8",
		"doctrine/migrations": "^3.1",
		"friendsofphp/php-cs-fixer": "^3.13",
		"nette/bootstrap": "^3.1",
		"nette/tester": "^2.4.3",
		"phpstan/phpstan": "^1.9",
		"phpstan/phpstan-nette": "^1.1",
		"roave/security-advisories": "dev-latest"
	},
	"suggest": {
		"nettrine/orm": "For integration with nettrine/orm",
		"nettrine/migrations": "For integration with nettrine/migrations"
	},
	"autoload": {
		"psr-4": {
			"SixtyEightPublishers\\DoctrineBridge\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SixtyEightPublishers\\DoctrineBridge\\Tests\\": "tests/"
		}
	},
	"extra": {
		"bamarni-bin": {
			"bin-links": false,
			"forward-command": true
		},
		"merge-plugin": {
			"merge-dev": true
		}
	},
	"config": {
		"allow-plugins": {
			"bamarni/composer-bin-plugin": true
		},
		"sort-packages": true
	}
}
